modulepath: "/etc/puppetlabs/code/environments/simple/modules:/etc/puppetlabs/code/environments/production/modules"
concurrency: 10
format: human
ssh:
  host-key-check: false
  private-key: /etc/ssh/simple_host_key
  user: root
log:
  console:
    level: info
  /root/.puppetlabs/bolt/debug.log:
    level: debug
    append: false  
